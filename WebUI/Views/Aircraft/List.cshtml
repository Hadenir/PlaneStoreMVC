@model AircraftListViewModel

@foreach (var aircraft in Model.Aircraft)
{
    @await Html.PartialAsync("AircraftSummary", aircraft);
}

<div page-action="List" page-model="@Model.PagingInfo"
    class="btn-group pull-right m-1"
    page-class="btn"
    page-class-normal="btn-secondary"
    page-class-selected="btn-primary">
</div>